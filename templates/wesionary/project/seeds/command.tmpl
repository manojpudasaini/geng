package seeds

// Seed db seed
type Seed interface {
	// name is used to identify the seed
	Name() string
	Setup()
}

// Seeds listing of seeds
type Seeds []Seed

// Run run the seed data
func (s Seeds) Setup() {
	for _, seed := range s {
		seed.Setup()
	}
}

func NewSeeds() Seeds {
	return Seeds{}
}
